<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Books List</title>
    <style>
        table, th, td {
            border: 1px solid black;
        }
    </style>
</head>
<body>
    <h1>Books List</h1>
    <table>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Author</th>
            <th>Year Written</th>
            <th>Edition</th>
            <th>Price</th>            
        </tr>
        <% books.forEach(item => { %>
            <tr>
                <td><%= item.id %></td>
                <td><%= item.title %></td>
                <td><%= item.author %></td>     
                <td><%= item.year_written %></td>     
                <td><%= item.edition %></td>     
                <td><%= item.price %></td>     
            </tr>
        <% }) %>
    </table>
</body>
</html>